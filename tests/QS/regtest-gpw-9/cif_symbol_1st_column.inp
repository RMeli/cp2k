&GLOBAL
  PROJECT cif_symbol_1st_column
  PRINT_LEVEL MEDIUM
  RUN_TYPE ENERGY
&END GLOBAL
&FORCE_EVAL
  METHOD Quickstep
  &SUBSYS
    &CELL
      CELL_FILE_FORMAT CIF
      CELL_FILE_NAME symbol_1st_column.cif
    &END CELL
    &TOPOLOGY
      COORD_FILE_FORMAT CIF
      COORD_FILE_NAME symbol_1st_column.cif
    &END TOPOLOGY
    &KIND H
      BASIS_SET ORB SZV-MOLOPT-SR-GTH
      POTENTIAL GTH-PBE
    &END KIND
    &KIND O
      BASIS_SET ORB SZV-MOLOPT-SR-GTH
      POTENTIAL GTH-PBE
    &END KIND
  &END SUBSYS
  &DFT
    BASIS_SET_FILE_NAME  BASIS_MOLOPT
    POTENTIAL_FILE_NAME  GTH_POTENTIALS
    &XC
      &XC_FUNCTIONAL PBE
      &END XC_FUNCTIONAL
    &END XC
    &SCF
      SCF_GUESS MOPAC
      MAX_SCF  1
      &PRINT
        &RESTART OFF
        &END
      &END
    &END SCF
  &END DFT
&END FORCE_EVAL
